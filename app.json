{
  "expo": {
    "name": "cardity",
    "slug": "cardity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Cardity.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Cardity2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      },
      "bundleIdentifier": "com.shadeonline.cardity"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Cardity2.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/Cardity.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "936e9904-d124-4783-8389-9aeb222922f5"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/936e9904-d124-4783-8389-9aeb222922f5"
    }
  }
}
